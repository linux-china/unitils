<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="aBeanName" class="java.lang.String">
    	<constructor-arg type="java.lang.String" index="0" value="aBeanValue"/>
    </bean>

    <bean id="testBeanByType" class="java.util.ArrayList"/>

    <bean id="testBeanByName" class="java.lang.String"/>

    <bean id="testBeanByTypeAmbiguous1" class="java.util.HashSet"/>
    <bean id="testBeanByTypeAmbiguous2" class="java.util.TreeSet"/>

</beans>
